{
  "status": 0,
  "msg": "",
  "doNotDisplayToast": 0,
  "_debug": {},
  "data": {
    "type": "page",
    "body": {
      "type": "flex",
      "items": [
        {
          "type": "card",
          "className": "w-1\/4 mr-5 mb-0",
          "body": [
            {
              "name": "static",
              "type": "static",
              "label": "指标域"
            },
            {
              "type": "form",
              "id": "treeForm",
              "wrapWithPanel": false,
              "body": {
                "type": "input-tree",
                "name": "selectedNode",
                "source": "\/amis\/metric-catalogs",
                "labelField": "name",
                "valueField": "id",
                "showIcon": false,
                "onEvent": {
                  "change": {
                    "actions": [
                      {
                        "actionType": "reload",
                        "componentId": "rightTable",
                        "args": {
                          "selectedNode": "${event.data.selectedNode}"
                        }
                      }
                    ]
                  }
                }
              }
            }
          ]
        },
        {
          "type": "page",
          "className": "m:overflow-auto",
          "body": [
            [],
            {
              "type": "crud",
              "id": "rightTable",
              "name": "rightTable",
              "placeholder": "请先选择左侧目录",
              "perPage": 20,
              "alwaysShowPagination": true,
              "affixHeader": false,
              "filterTogglable": false,
              "filterDefaultVisible": false,
              "api": "\/amis\/metrics?businessCatalog=${selectedNode}",
              "bulkActions": [
              ],
              "perPageAvailable": [
                10,
                20,
                50
              ],
              "footerToolbar": [],
              "headerToolbar": [
              ],
              "primaryField": "id",
              "columns": [
                {
                  "name": "id",
                  "label": "ID"
                },
                {
                  "name": "cnName",
                  "label": "中文名称"
                },
                {
                  "name": "enName",
                  "label": "英文名称"
                },
                {
                  "name": "serialNumber",
                  "label": "指标编码"
                },
                {
                  "name": "${dataset.name}",
                  "label": "数据集"
                },
                {
                  "name": "${businessOwners[0].name}",
                  "label": "业务负责人"
                },
                {
                  "name": "${techOwners[0].name}",
                  "label": "技术负责人"
                },
                {
                  "type": "select",
                  "name": "pubsub",
                  "label": "是否上线",
                  "disabled": true,
                  "options": [
                    {
                      "label": "未上线",
                      "value": 0
                    },
                    {
                      "label": "已发布",
                      "value": 1
                    }
                  ]
                },
                {
                  "type": "operation",
                  "label": "\u64cd\u4f5c",
                  "buttons": [
                    {
                      "type": "button",
                      "actionType": "dialog",
                      "dialog": {
                        "type": "dialog",
                        "title": "试算",
                        "body": {
                          "type": "page",
                          "body": [
                            {
                              "type": "service",
                              "className": "mb-4",
                              "title": "试算SQL",
                              "api": "\/amis\/metrics\/${id}\/previewSql",
                              "body": {
                                "type": "textarea",
                                "name": "previewSql",
                                "value": "${content}",
                                "disabled": true,
                                "minRows": 10,
                                "maxRows": 15
                              }
                            },
                            {
                              "type": "crud",
                              "title": "试算数据",
                              "className": "mt-4",
                              "api": "\/amis\/query\/metrics\/${id}\/preview"
                            }
                          ]
                        },
                        "size": "xl"
                      },
                      "label": "试算",
                      "icon": "fa-solid fa-calculator",
                      "level": "link",
                      "hiddenOn": "${type == 1 || type == 2 }"
                    }
                  ]
                }
              ]
            },
            [],
            [],
            []
          ]
        }
      ]
    }
  }
}